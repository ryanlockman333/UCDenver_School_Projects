\pard\plain\ltrpar\ql\s27\sl240\slmult1\itap0{\s27\f1\fs24\lang1033{\*\listtag0}\abinodiroverride\ltrch Parking East Parking Lot- Parking Space 201}{\s27\f1\fs24\lang1033{\*\listtag0}\par}
